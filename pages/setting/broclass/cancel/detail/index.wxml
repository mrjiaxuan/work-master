<!--pages/home/cancel/detail/index.wxml-->
<view class="main">
  <view class="bd">
    <view>
        <text class='title' decode='{{true}}'>&nbsp;&nbsp;{{failureTime}}</text>
      </view>
      <view>
        <text class='title' decode='{{true}}'>&nbsp;&nbsp;用途:</text>
        <text class='content'>  {{queryBean.usages}}</text>
      </view>
      <view>
        <text class='title' decode='{{true}}'>&nbsp;&nbsp;备注:</text>
        <text class='content'>  {{queryBean.remark}}</text>
      </view>
      <view>
        <text class='title' decode='{{true}}'>&nbsp;&nbsp;驳回原因:</text>
        <text class='content'>  {{failureReason}}</text>
      </view>
  </view>
</view>
